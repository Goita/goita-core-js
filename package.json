        "tslib": "^1.7.1"
    },
    "devDependencies": {
        "@types/chai": "^4.0.1",
        "@types/mocha": "^2.2.41",
        "@types/node": "^8.0.7",
        "chai": "^4.0.2",
        "mocha": "^3.4.2",
        "ts-node": "^3.1.0",
        "tslint": "^4.0.2",
        "typescript": "^2.4.1",
        "webpack": "^2.2.1"
